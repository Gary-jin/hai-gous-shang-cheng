{
  "name": "avvw",
  "version": "1.3.0",
  "description": "Apicloud + Vue2 + Vant（有赞前端）+ Webpack(Vue-cli3)打包，是一个采用Vue数据绑定特性和Apicloud手机操控能力相结合的APP开发框架",
  "scripts": {
    "wifi-start": "apicloud wifiStart --port 23457",
    "wifi-stop": "apicloud wifiStop --port 23457",
    "wifi-sync": "apicloud wifiSync --project ./dist --updateAll false --port 23457",
    "wifi-log": "apicloud wifiLog --port 23457",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=8192 vue-cli-service serve",
    "test": "vue-cli-service build --mode test",
    "build": "vue-cli-service build",
    "build-modern": "vue-cli-service build --modern",
    "webpack-config": "vue-cli-service inspect > webpack.js"
  },
  "author": "Marlon <124711483@qq.com>",
  "license": "MIT",
  "keywords": [
    "avvw",
    "apicloud",
    "vue",
    "vue2",
    "vant",
    "webpack",
    "app",
    "hybird",
    "ios",
    "android",
    "javascript",
    "nodejs",
    "vue3-cli"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/grapewheel/avvw"
  },
  "dependencies": {
    "js-time.js": "^1.3.14",
    "vant": "^2.10.8",
    "vue": "^2.6.10",
    "vue-async-computed": "^3.7.0",
    "vue2-touch-events": "^2.2.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "apicloud-cli": "^0.2.0",
    "babel-plugin-import": "^1.12.2",
    "cross-env": "^7.0.2",
    "klaw-sync": "^6.0.0",
    "node-sass": "^4.14.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.10"
  }
}
